# =================================
# Environment Service Development
# =================================
NODE_ENV=development
PORT=3001

# Database
DATABASE_URL=postgresql://kasm:kasm@localhost:5432/kasm

# Redis
REDIS_URL=redis://localhost:6379

# Kubernetes Configuration (for dev, you might want to use minikube)
KUBE_CONFIG_PATH=/home/user/.kube/config
KUBE_NAMESPACE=kasm-dev

# Resource Limits
DEFAULT_CPU_LIMIT=100m
DEFAULT_MEMORY_LIMIT=128Mi
DEFAULT_STORAGE_LIMIT=1Gi